{
  "schema_version": 1,
  "kind": "opensspm.dataset_contract",
  "dataset": {
    "key": "okta:authenticators",
    "version": 1,
    "description": "Okta authenticators (for example: Okta Verify, Smart Card, Password).",
    "primary_key": "/id",
    "recommended_display": "/name",
    "schema": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "id": { "type": "string", "description": "Authenticator identifier." },
        "key": { "type": "string", "description": "Authenticator key (vendor-defined)." },
        "name": { "type": "string", "description": "Authenticator name." },
        "status": { "type": "string", "description": "Authenticator status (vendor-defined)." },
        "settings": { "type": "object", "additionalProperties": true }
      },
      "required": ["id"]
    }
  }
}
